{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "PermittedRedirects": {
      "admin_homepage": "https://admin.jibberwock.com",
      "status_report": "https://app.powerbi.com"
    },
    "EasyAuth": {
      "IdProvider": "aad"
    },
    "Authorization": {
      "DefaultServiceId": 1
    },
    "ServiceBus": {
      "Namespace": "jibberwock-bus",
      "Queues": {
      }
    },
    "SendGrid": {
      "EmailIdParameterName": "jibberwock_email_id",
      "NotificationIdParameterName": "jibberwock_notification_id"
    },
    "AppInsightsConfiguration": {
      "ExceptionTimeRange": "31.00:00:00"
    },
    "SensitiveSettingKeyVaultName": "jibberwock-credentials"
  }
}
